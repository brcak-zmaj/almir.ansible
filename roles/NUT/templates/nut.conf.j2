# {{ ansible_managed }}
# =============================================================================
# NUT Configuration File - /etc/nut/nut.conf
# =============================================================================
#
# This file configures the NUT operating mode:
#   none       - NUT is not configured
#   standalone - UPS connected locally, no network sharing
#   netserver  - UPS connected locally, shares over network
#   netclient  - Monitors a remote NUT server
#

MODE={{ nut_mode }}
